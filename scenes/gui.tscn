[sub_resource type="GDScript" id="GDScript_ynd5l"]
script/source = "extends CanvasLayer

@onready var menu_container = $MenuContainer
@onready var animation_player: AnimationPlayer = $AnimationPlayer

func _ready() -> void:
	Game.ON_MENU_CLOSE.connect(self.on_menu_close)
	animation_player.animation_finished.connect(self._on_animation_finished)
	animation_player.animation_started.connect(self._on_animation_started)

func _input(event: InputEvent) -> void:
	if event.is_action_pressed(\"ui_open\"):
		_play_animation()

func on_menu_close() -> void:
	animation_player.play(\"shop_close\")

func _play_animation() -> void:
	print(\"What is: %s\" % menu_container.visible)
	if not menu_container.visible:
		animation_player.play(\"shop_open\")
	else:
		animation_player.play(\"shop_close\")

func _on_animation_started(anim_name: String) -> void:
	if anim_name == \"shop_open\":
		menu_container.visible = true
	
	Game.updateMouse(menu_container.visible)

func _on_animation_finished(anim_name: String) -> void:
	if anim_name == \"shop_close\":
		menu_container.visible = false
	
	Game.updateMouse(menu_container.visible)
"

